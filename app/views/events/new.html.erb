
<div class="new_event_main">
<div class="side-bar">
    <%= render partial: "side_bar",locals: {p: @p} %>
  </div>
  <div class="event_color">
<div class="new_event">

<%= form_with(model: @event,url:events_path, local: true) do |form| %>


  <div class="event_tie">
  <h3>title</h3>
   
    <%= form.text_field :title,class: :event_text %>
  </div>

    <div class="event_time">
    <h3>日時</h3>
    <%= form.datetime_field :start_time,class: :event_date %>
  </div>

  <div class="event_sche">
    <h3>活動内容</h3>
    <%= form.text_area :content,class: :event_schedule %>
  </div>


  <div class="sche">
    <%= form.submit %>
  </div>


  

<% end %>
  </div>
